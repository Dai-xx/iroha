{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "no-restricted-imports": ["error", { "patterns": ["../"] }],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "type",
          "internal",
          ["parent", "sibling"],
          "object",
          "index"
        ]
      }
    ]
  }
}
